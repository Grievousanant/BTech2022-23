    "# fibonacci nth element\n",
    "def fibonacci(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    return fibonacci(n-1) + fibonacci(n-2)\n",
    "\n",
    "# main code\n",
    "n = 6\n",
    "out = fibonacci(n)\n",
    "print(out)"
   ]
  }
