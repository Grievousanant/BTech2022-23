   "source": [
    "# The Ship Team\n",
    "dct = eval(input())\n",
    "team1 = []\n",
    "team2 = []\n",
    "for i in dct:\n",
    "    if dct[i] < 20 or dct[i] > 40:\n",
    "        team1.append(i)\n",
    "    else:\n",
    "        team2.append(i)\n",
    "\n",
    "team1.sort()\n",
    "team2.sort()\n",
    "out = f'[\\n {team1},\\n {team2}\\n]'\n",
    "print(out)"
   ]
  }
